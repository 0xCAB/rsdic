def configure(ctx):
  pass

def build(bld):
  bld.program(
       source       = 'PerformanceTest.cpp',
       target       = 'PerformanceTest',
       use          = 'RSDIC',
       includes     = '.')
  bld.program(
       source       = 'CombinationGenerator.cpp',
       target       = 'CombinationGenerator',
       use          = '',
       includes     = '.')
